{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "4.2.*",
		"laravelbook/ardent": "dev-master",
		"gidlov/copycat": "dev-master",
		"guzzlehttp/guzzle": "~5.1",
		"mitch/hashids": "1.x",
		"laravel/cashier": "~2.0",
		"ignited/laravel-omnipay": "1.*",
		"omnipay/paypal": "~2.0",
		"omnipay/stripe": "~2.0",
		"indatus/dispatcher": "~1.4",
		"guzzle/guzzle": "~3.9",
		"paypal/adaptivepayments-sdk-php": "~3.8",
		"intervention/image": "~2.1"
	},
	"require-dev": {
		"way/generators": "~2.0",
		"mockery/mockery": "dev-master@dev",
		"fzaninotto/faker": "1.4.*@dev",
		"league/factory-muffin": "~2.0"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/lib",
			"vendor/paypal"
		],
		"files": [
			"vendor/gidlov/copycat/src/Gidlov/Copycat/Copycat.php",
			"vendor/paypal/sdk-core-php/tests/PPBootStrap.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
